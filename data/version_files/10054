                                    <rdf:Description rdf:about="&CGI_Lith;highMetamorphicGrade"/>
                                    <rdf:Description rdf:about="&CGI_Lith;lowMetamorphicGrade"/>
                                    <rdf:Description rdf:about="&CGI_Lith;mediumMetamorphicGrade"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ParticleType"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;crystal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>A schist that consists of more than 50 percent mica minerals, typically muscovite or biotite. Special type included to distinguish this common variety of schist.</dc:description>
    </owl:Class>
    


