    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/blueschist_mineralogy -->

    <owl:Class rdf:about="&CGI_Lith;blueschist_mineralogy">
        <rdfs:label>glaucophane lawsonite epidote mineralogy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;AnyConstituent"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;calcFerruginousSilicate"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;carbonate"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;hybridSedimentaryConstituents"/>
        <dc:description>A metamorphic rock of roughly basaltic composition, defined by the presence of glaucophane with lawsonite or epidote. Other minerals that may be present include jadeite, albite, chlorite, garnet, and muscovite (phengitic white mica). Typically fine-grained, dark colored. Category for rocks commonly referred to as blueschist.</dc:description>
    </owl:Class>
    


