        <rdfs:label>calcareous carbonate mineralogy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;carbonate"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;magnesianCarbonate"/>
        <dc:description>Ratio magnesium carbonate to calcite (plus aragonite) in the carbonate mineral fraction is less than 1 to 1</dc:description>
    </owl:Class>
    


