    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/ChemCat -->

    <owl:ObjectProperty rdf:about="&CGI_Lith;ChemCat">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>chemical category</rdfs:label>
        <rdfs:comment>property that specifies one or more chemical categories for an earth material. Chemical category assignment requires a chemical analysis</rdfs:comment>
        <rdfs:range rdf:resource="&CGI_Lith;AnyChemCat"/>
        <rdfs:domain rdf:resource="&CGI_Lith;rock_material"/>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/ConsolDegree -->

    <owl:ObjectProperty rdf:about="&CGI_Lith;ConsolDegree">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>consolidation degree</rdfs:label>
        <rdfs:range rdf:resource="&CGI_Lith;AnyConsolDegree"/>
        <rdfs:domain rdf:resource="&CGI_Lith;rock_material"/>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/Constituents -->

    <owl:ObjectProperty rdf:about="&CGI_Lith;Constituents">
        <rdfs:label>constituent part</rdfs:label>
        <dc:description>property links to terms that are implemented by GeoSciML CompoundMaterial.constituent.ConstituentPart elements. The property value vocabulary specifies mineralogic composition like location in Streckeisen (1978) QAPF space, mafic mineral content (color index),  NADM metamorphic (2004) mineralogic composition categories, or sandstone QFL composition. The ConstituentPart elements represent collections of particles that have the same material composition or particleType. The material may be any kind of EarthMaterial (mineral, rock, fluid in pores). ParticleTypes are used to specify generic kinds of particle constituents like intraclasts, bioclasts, pyroclastic particle, etc.</dc:description>
        <rdfs:range rdf:resource="&CGI_Lith;AnyConstituent"/>
        <rdfs:domain rdf:resource="&CGI_Lith;rock_material"/>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/FabricType -->

    <owl:ObjectProperty rdf:about="&CGI_Lith;FabricType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>fabric type</rdfs:label>
        <rdfs:range rdf:resource="&CGI_Lith;AnyFabricType"/>
        <rdfs:domain rdf:resource="&CGI_Lith;rock_material"/>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/GeneticCateg -->

    <owl:ObjectProperty rdf:about="&CGI_Lith;GeneticCateg">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>genetic category</rdfs:label>
        <rdfs:range rdf:resource="&CGI_Lith;AnyGeneticCateg"/>
        <rdfs:domain rdf:resource="&CGI_Lith;rock_material"/>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/GrainSize -->

    <owl:ObjectProperty rdf:about="&CGI_Lith;GrainSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
