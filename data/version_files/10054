                                    <rdf:Description rdf:about="&CGI_Lith;lowMetamorphicGrade"/>
                                    <rdf:Description rdf:about="&CGI_Lith;mediumMetamorphicGrade"/>
                                    <rdf:Description rdf:about="&CGI_Lith;veryLowMetamorphicGrade"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ParticleType"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&CGI_Lith;crystal"/>
                                    <rdf:Description rdf:about="&CGI_Lith;grain"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


