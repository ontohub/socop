    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/carbonate -->

    <owl:Class rdf:about="&CGI_Lith;carbonate">
        <rdfs:label>carbonate mineralogy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;AnyConstituent"/>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;exotic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;carbonateBearing"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;chlorite_actinolite_epidote_mineralogy"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;eclogitic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;glass_rich"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;hybridSedimentaryConstituents"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;ironRich"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;kalsilitic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;kimberliteMineralogy"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;lamproiteMineralogy"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;lamprophyreMineralogy"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;melilitic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;pelitic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;quartzose"/>
        <dc:description>consists of greater than 50 percent carbonate minerals. This is an exotic category for igneous rocks only.</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/carbonateBearing -->

    <owl:Class rdf:about="&CGI_Lith;carbonateBearing">
        <rdfs:label xml:lang="en">carbonate bearing mineralogy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;AnyConstituent"/>
        <dc:description>contains between 10 and 50 percent carbonate minerals in the mud size fraction.  Maintainence of size criteria is tricky for definition of carbonate rich mudstone</dc:description>
    </owl:Class>
    


