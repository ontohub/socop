                        <owl:allValuesFrom rdf:resource="&CGI_Lith;hypabyssal"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;GrainSize"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&CGI_Lith;fineGrained"/>
