    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/high_magnesium_fine_grained_igneous_rock -->

    <owl:Class rdf:about="&CGI_Lith;high_magnesium_fine_grained_igneous_rock">
        <rdfs:label>high-magnesium fine-grained igneous rock</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&CGI_Lith;rock_material"/>
