    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/exotic -->

    <owl:Class rdf:about="&CGI_Lith;exotic">
        <rdfs:label>exotic igneous mineralogy</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;AnyConstituent"/>
        <dc:description>Unusual igneous rock mineral composition. Mineralogy is carbonate mineral dominated, kimberlite, lamproite, or lamprophyre. Olivine (commonly serpentinized in kimberlite), and phlogopite are significant constituents except in carbonate mineral dominant.</dc:description>
    </owl:Class>
    


