    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/silt_size -->

    <owl:Class rdf:about="&CGI_Lith;silt_size">
        <rdfs:label xml:lang="en">silt-size</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;mud_size"/>
        <dc:description xml:lang="en">Greater than 50 percent particles have a grain size between 0.004 mm and 0.032 mm (Wentworth 1922), but this determination can only be made in unconsolidated material that can be analyzed by centrifuge. In consolidated material, the operational criteria is that the material contains significant detectable silt.</dc:description>
    </owl:Class>
    


