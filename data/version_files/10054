    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/travertine -->

    <owl:Class rdf:about="&CGI_Lith;travertine">
        <rdfs:label>travertine</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&CGI_Lith;rock_material"/>
                    <owl:Restriction>
