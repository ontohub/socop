    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/dioritoid -->

    <owl:Class rdf:about="&CGI_Lith;dioritoid">
        <rdfs:label>dioritoid</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&CGI_Lith;rock_material"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ChemCat"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;intermediate"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ConsolDegree"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;consolidated"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;Constituents"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;QAPF9_10"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;Constituents"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;an_LTE_50"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;GeneticCateg"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;plutonic"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;GrainSize"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;phaneritic"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ParticleType"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;crystal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;rock_material"/>
        <dc:description>Phaneritic crystalline igneous rock with M less than 90, consisting of intermediate plagioclase, commonly with hornblende and often with biotite or augite. Plagioclase to total feldspar ratio is greater that 0.65, and anorthite content of plagioclase is less than 50 percent. Less than 10 percent feldspathoid mineral and less than 20 percent quartz in the QAPF fraction.  Includes rocks defined modally in QAPF fields 9 and 10 (and their subdivisions).</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/doleritic_rock -->

