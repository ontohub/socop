    <owl:Class rdf:about="&CGI_Lith;CalciumSulfateMineralogy">
        <rdfs:subClassOf rdf:resource="&CGI_Lith;nonCarbonateSaltMineralogy"/>
        <dc:description>consists of more than 50 percent gypsum or anhydrite</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT0_LT10 -->

    <owl:Class rdf:about="&CGI_Lith;F_GT0_LT10">
        <rdfs:subClassOf rdf:resource="&CGI_Lith;M_LT90"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;F_GT10_LT60"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;F_GT60"/>
        <dc:description>Feldspathoid minerals form between 0 and 10 percent of QAPF fraction</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT10_LT60 -->

    <owl:Class rdf:about="&CGI_Lith;F_GT10_LT60">
