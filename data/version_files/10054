    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/ultrabasic_igneous_rock -->

    <owl:Class rdf:about="&CGI_Lith;ultrabasic_igneous_rock">
        <rdfs:label>ultrabasic igneous rock</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&CGI_Lith;rock_material"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ChemCat"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;ultraBasic"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ConsolDegree"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;consolidated"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;GeneticCateg"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;igneous"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>igneous rock that contains less than 45 percent silica.</dc:description>
        <rdfs:comment>Note that based on analyitcal data reported by EarthChem (12/28/09), most of the rock types generally considered ultraBasic have reported silica contents between 52 and about 40 percent...</rdfs:comment>
    </owl:Class>
    


