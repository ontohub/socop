    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/coal -->

    <owl:Class rdf:about="&CGI_Lith;coal">
        <rdfs:label>coal</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&CGI_Lith;rock_material"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ChemCat"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;carbonaceous"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ConsolDegree"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;consolidated"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;Constituents"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;lowMoistureOrganicMaterial"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;GeneticCateg"/>
                        <owl:allValuesFrom rdf:resource="&CGI_Lith;sedimentary"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&CGI_Lith;ParticleType"/>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&CGI_Lith;grain"/>
                                    <rdf:Description rdf:about="&CGI_Lith;noParticles"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>A consolidated organic sedimentary material having less than 75% moisture. This category includes low, medium, and high rank coals according to International Classification of In-Seam Coal (United Nations, 1998), thus including lignite. Sapropelic coal is not distinguished in this category from humic coals. Formed from the compaction or induration of variously altered plant remains similar to those of peaty deposits.</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/coarseGrainedPyroclastic -->

    <owl:Class rdf:about="&CGI_Lith;coarseGrainedPyroclastic">
        <rdfs:label>coarse grained pyroclastic</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;AnyGrainSize"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;fineGrained"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;fineGrainedPyroclastic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;glassy"/>
        <dc:description xml:lang="en">Rock in which greater than 25 percent of rock by volume are pyroclasts that are more than 64 mm in longest diameter</dc:description>
        <rdfs:comment xml:lang="en">technically not quite disjoint with sand_size. A rock might contain 27 percent fragments greater than 64 mm, and the rest be less than 2 mm...</rdfs:comment>
    </owl:Class>
    


