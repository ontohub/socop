                        <owl:allValuesFrom rdf:resource="&CGI_Lith;grain"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;rock_material"/>
        <dc:description>defined only based on gravel-size grain size;  constituent type or sedimentary genesis not specified</dc:description>
    </owl:Class>
    


