    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/clay_size -->

    <owl:Class rdf:about="&CGI_Lith;clay_size">
        <rdfs:label xml:lang="en">clay-size</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;mud_size"/>
        <dc:description xml:lang="en">Technically defined to mean greater than 50 percent particles have a grain size less than 0.004 mm (Wentworth 1922), but this determination can only be made in unconsolidated material that can be analyzed by centrifuge. In consolidated material, the operational criteria is that the material contains no detectable silt, and is inferred to consist virtually entirely of clay-size particles.</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/claystone -->

