    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="&CGI_Lith;F_GT0_LT10"/>
            <rdf:Description rdf:about="&CGI_Lith;F_GT10_LT60"/>
            <rdf:Description rdf:about="&CGI_Lith;F_GT60"/>
            <rdf:Description rdf:about="&CGI_Lith;quartzFeldsparPeliticMineralogy"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="&CGI_Lith;blueschist_mineralogy"/>
            <rdf:Description rdf:about="&CGI_Lith;calcFerruginousSilicate"/>
            <rdf:Description rdf:about="&CGI_Lith;carbonate"/>
            <rdf:Description rdf:about="&CGI_Lith;quartzFeldsparPeliticMineralogy"/>
        </owl:members>
    </rdf:Description>
    <rdf:Description>
