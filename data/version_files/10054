        <owl:disjointWith rdf:resource="&CGI_Lith;amphibolitic"/>
        <dc:description>Feldspathoid minerals form between 10 and 60 percent of QAPF fraction</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT60 -->

    <owl:Class rdf:about="&CGI_Lith;F_GT60">
        <rdfs:subClassOf rdf:resource="&CGI_Lith;M_LT90"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;amphibolitic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;hybridSedimentaryConstituents"/>
        <dc:description>Feldspathoid minerals form greater than  60 percent of QAPF fraction</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT60_LT90 -->

    <owl:Class rdf:about="&CGI_Lith;F_GT60_LT90">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&CGI_Lith;F_GT60"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="&CGI_Lith;F_GT90"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;F_GT60"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;F_GT90"/>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/F_GT90 -->

    <owl:Class rdf:about="&CGI_Lith;F_GT90">
        <owl:equivalentClass rdf:resource="&CGI_Lith;QAPF15c"/>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;F_GT60"/>
        <dc:description>Feldspathoid minerals form greater than 90 percent of QAPF fraction</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/M_GT10_LT90 -->

    <owl:Class rdf:about="&CGI_Lith;M_GT10_LT90">
        <rdfs:label>Between 10 and 90 percent modal mafic minerals</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;M_LT90"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;M_LTE_10"/>
        <dc:description>Mafic minerals form between 10 and 90 percent of rock.</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/M_LT90 -->

    <owl:Class rdf:about="&CGI_Lith;M_LT90">
        <rdfs:label>Normal composition igneous rock</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;AnyConstituent"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;calcFerruginousSilicate"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;exotic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;glass_dominant"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;ultramafic"/>
        <rdfs:comment>Mafic minerals form less than 90 percent of rock. Category for normal-composition igneous rocks, dominated by quartz, feldspar, and feldspathoid minerals, with compositions determined by fields in the Streckeisen 1978 QAPF triangles.</rdfs:comment>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/M_LTE_10 -->

    <owl:Class rdf:about="&CGI_Lith;M_LTE_10">
        <rdfs:label>Less than 10 percent modal mafic minerals</rdfs:label>
        <rdfs:subClassOf rdf:resource="&CGI_Lith;M_LT90"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;amphibolitic"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;mafic"/>
        <dc:description>Mafic minerals form less than 10 percent of rock.</dc:description>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/classifier/cgi/lithOntology/P_A_GT10_LT35 -->

    <owl:Class rdf:about="&CGI_Lith;P_A_GT10_LT35">
        <rdfs:subClassOf rdf:resource="&CGI_Lith;M_LT90"/>
